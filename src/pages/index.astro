---
import "../styles/global.css";
import Layout from "../layouts/Layout.astro";
import PresentCarrera from "src/components/inicio/PresentCarrera.astro";
import Eventos from "src/components/inicio/Eventos.astro";
import Patrocinadores from "src/components/inicio/Patrocinadores.astro";
import { getCollection } from "astro:content";

const [staticData] = await getCollection("staticData");
const { pageTitle, pageDescription, OGImage } = staticData.data;
---

<Layout
  pageTitle={pageTitle}
  description={pageDescription}
  ogimage={{ alt: OGImage.alt, url: OGImage.url }}
>

  <PresentCarrera />
  <Eventos />
  <Patrocinadores />
  <!-- <HeroIndex /> -->
  <!-- <Experience /> -->
  <!-- <ListProjects /> -->
</Layout>
