{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "src/**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": ".",
    "paths": {
      "congreso:componentes":["src/components/congreso/index.ts"],
      "congreso:layouts":["src/layouts/congreso/index.ts"],
      "congreso:ui": [
        "src/components/ui/congreso/index.ts"
      ],
      "congreso:styles/*":["src/styles/congreso/*"],
      "congreso:types":["src/components/types/congreso.ts"]

    }

  }
}